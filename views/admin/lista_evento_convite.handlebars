  <table class="table table-hover mt-3 mb-2 display" id="tabela-eventos" style="margin-top: 100px;">
    <thead class="table-dark">
      <tr>
        <th>Tipo</th>
        <th>Data</th>
        <th>Horário Início</th>
        <th>Horário Término</th>
        <th style="display:none"></th>
        <th>Descrição</th>
        <th>Local</th>
        <th></th>
      </tr>
    </thead>
    <tbody>
      {{#each eventos}}
        <tr>
          <td>{{tipo}}</td>
          <td>{{data}}</td> 
          <td>{{horarioinicio}}</td>
          <td>{{horariotermino}}</td>
          <td style="display:none">{{descricao}}</td>
          <td>{{descricao}}</td>
          <td>{{local}}</td>
          <td><a href="/admin/seleciona_evento_convite/{{_id}}"><i class="fa-solid fa-envelope-open-text"></i></a></td>
        </tr>
      {{/each}}
    </tbody>
  </table>